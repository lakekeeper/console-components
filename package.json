{
  "name": "@lakekeeper/console-components",
  "version": "0.1.2",
  "description": "A library of Vue components for Lakekeeper console applications",
  "type": "module",
  "main": "dist/console-components.umd.js",
  "module": "dist/console-components.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/console-components.es.js",
      "require": "./dist/console-components.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint": "eslint . --fix",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "publish:npm": "npm publish",
    "publish:dry-run": "npm publish --dry-run",
    "format": "prettier --write .",
    "prepare": "npm run build",
    "format:check": "prettier --check .",
    "lint:check": "eslint ."
  },
  "keywords": [
    "vue",
    "vuetify",
    "components",
    "library",
    "lakekeeper",
    "console"
  ],
  "author": "Lakekeeper",
  "license": "Apache-2.0",
  "peerDependencies": {
    "vue": "^3.5.16",
    "vuetify": "^3.8.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@hey-api/client-fetch": "^0.10.2",
    "@hey-api/openapi-ts": "^0.66.7",
    "@types/chart.js": "^2.9.41",
    "@types/json-bigint": "^1.0.4",
    "@vitejs/plugin-vue": "^5.0.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^9.33.0",
    "globals": "^15.15.0",
    "jest": "^29.0.0",
    "json-bigint": "^1.0.0",
    "oidc-client-ts": "^3.3.0",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate-2": "^2.0.31",
    "prettier": "^3.6.2",
    "sass": "^1.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.45.0",
    "vite": "^5.0.0",
    "vite-plugin-vuetify": "^2.1.1",
    "vue-json-pretty": "^2.5.0",
    "vue-router": "^4.5.1",
    "vue-tsc": "^2.0.0"
  },
  "dependencies": {
    "@duckdb/duckdb-wasm": "^1.31.1-dev1.0",
    "@wdns/vue-code-block": "^2.3.5",
    "apache-arrow": "^21.1.0",
    "chart.js": "^4.4.9",
    "date-fns": "^4.1.0",
    "vue-chartjs": "^5.3.2"
  }
}
