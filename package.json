{
  "name": "@lakekeeper/console-components",
  "version": "0.0.1",
  "description": "A library of Vue components for Lakekeeper console applications",
  "type": "module",
  "main": "dist/console-components.umd.js",
  "module": "dist/console-components.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/console-components.es.js",
      "require": "./dist/console-components.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "dev": "vite",
    "lint": "eslint .",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "publish:npm": "npm publish",
    "publish:dry-run": "npm publish --dry-run"
  },
  "keywords": [
    "vue",
    "vuetify",
    "components",
    "library",
    "lakekeeper",
    "console"
  ],
  "author": "Lakekeeper",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.5.16",
    "vuetify": "^3.8.7"
  },
  "devDependencies": {
    "@hey-api/client-fetch": "^0.10.2",
    "@hey-api/openapi-ts": "^0.66.7",
    "@vitejs/plugin-vue": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "sass": "^1.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-vuetify": "^2.1.1",
    "vue-tsc": "^2.0.0"
  }
}
